{
    "name": "remarkablemark/rector-laravel-service-mocking",
    "type": "library",
    "description": "Rector to replace deprecated Laravel service mocking testing methods",
    "keywords": [
        "rector",
        "refactor",
        "codemod",
        "laravel",
        "service",
        "mocking",
        "testing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark",
            "email": "mark@remarkablemark.org",
            "homepage": "https://remarkablemark.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Remarkablemark\\RectorLaravelServiceMocking\\": "src"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.53.0",
        "phpunit/phpunit": "10.5.19",
        "rector/rector": "1.0.4",
        "symplify/rule-doc-generator": "12.1.10"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "version": "2.0.0"
}
