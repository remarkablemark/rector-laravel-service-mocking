{
    "name": "remarkablemark/rector-laravel-service-mocking",
    "type": "library",
    "description": "Rector to replace deprecated Laravel service mocking testing methods",
    "keywords": [
        "rector",
        "refactor",
        "codemod",
        "laravel",
        "service",
        "mocking",
        "testing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark",
            "email": "mark@remarkablemark.org",
            "homepage": "https://remarkablemark.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "autoload": {
        "psr-4": {
            "Remarkablemark\\RectorLaravelServiceMocking\\": "src"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.87.1",
        "phpunit/phpunit": "10.5.27",
        "rector/rector": "2.1.6",
        "symplify/rule-doc-generator": "12.2.5"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "version": "3.0.0"
}
